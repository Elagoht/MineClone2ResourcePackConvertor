#!/bin/env bash
MC_TEXTURES=$1
MC_SOURCE=$2
DEST=$3

# Check if source is not empty
mkdir -p $MC_TEXTURES
if [ -z "$(ls -A $MC_TEXTURES)" ]; then
	echo The $MC_TEXTURES is empty
	exit 1
fi

# Create source folder and check it is empty
mkdir -p $MC_SOURCE
if ls -A1q $MC_SOURCE | grep -q "."; then
	echo The $MC_SOURCE is not empty
	exit 1
fi

# Create destination folder and check it is empty
mkdir -p $DEST
if ls -A1q $DEST | grep -q "."; then
	echo The $DEST is not empty
	exit 1
fi

# Remove unnecessary files
rm -rf $MC_TEXTURES/entity/villager/profession_level
rm -rf $MC_TEXTURES/entity/zombie_villager/profession_level
rm -rf $MC_TEXTURES/colormap
rm -rf $MC_TEXTURES/effect
rm -rf $MC_TEXTURES/font
rm -rf $MC_TEXTURES/gui
rm -rf $MC_TEXTURES/map
rm -rf $MC_TEXTURES/misc
rm -rf $MC_TEXTURES/mob_effect
rm -rf $MC_TEXTURES/painting
rm -rf $MC_TEXTURES/particle
rm -rf $MC_TEXTURES/trims

# Collect all textures
find $MC_TEXTURES -type f -name "*.png" -exec mv -f {} $MC_SOURCE \; || exit 2

# Move to destination folder with new names
mv -f $MC_SOURCE/carrots_stage0.png $DEST/farming_carrot_1.png
mv -f $MC_SOURCE/empty_armor_slot_leggings.png $DEST/mcl_inventory_empty_armor_slot_leggings.png
mv -f $MC_SOURCE/brown_stained_glass.png $DEST/mcl_core_glass_brown.png
mv -f $MC_SOURCE/green_stained_glass.png $DEST/mcl_core_glass_green.png
mv -f $MC_SOURCE/black_stained_glass.png $DEST/mcl_core_glass_black.png
mv -f $MC_SOURCE/gray_stained_glass.png $DEST/mcl_core_glass_gray.png
mv -f $MC_SOURCE/lime_stained_glass.png $DEST/mcl_core_glass_lime.png
mv -f $MC_SOURCE/magenta_stained_glass.png $DEST/mcl_core_glass_magenta.png
mv -f $MC_SOURCE/cyan_stained_glass.png $DEST/mcl_core_glass_cyan.png
mv -f $MC_SOURCE/purple_stained_glass.png $DEST/mcl_core_glass_purple.png
mv -f $MC_SOURCE/light_gray_stained_glass.png $DEST/mcl_core_glass_light_gray.png
mv -f $MC_SOURCE/yellow_stained_glass.png $DEST/mcl_core_glass_yellow.png
mv -f $MC_SOURCE/white_stained_glass.png $DEST/mcl_core_glass_white.png
mv -f $MC_SOURCE/orange_stained_glass.png $DEST/mcl_core_glass_orange.png
mv -f $MC_SOURCE/light_blue_stained_glass.png $DEST/mcl_core_glass_light_blue.png
mv -f $MC_SOURCE/pink_stained_glass.png $DEST/mcl_core_glass_pink.png
mv -f $MC_SOURCE/tnt_bottom.png $DEST/default_tnt_bottom.png
mv -f $MC_SOURCE/empty_armor_slot_helmet.png $DEST/mcl_inventory_empty_armor_slot_helmet.png
mv -f $MC_SOURCE/empty_armor_slot_boots.png $DEST/mcl_inventory_empty_armor_slot_boots.png
mv -f $MC_SOURCE/empty_armor_slot_chestplate.png $DEST/mcl_inventory_empty_armor_slot_chestplate.png
mv -f $MC_SOURCE/black_stained_glass_pane_top.png $DEST/xpanes_top_glass_black.png
mv -f $MC_SOURCE/potatoes_stage0.png $DEST/mcl_farming_potatoes_stage_0.png
mv -f $MC_SOURCE/beetroots_stage0.png $DEST/mcl_farming_beetroot_0.png
mv -f $MC_SOURCE/brown_stained_glass_pane_top.png $DEST/xpanes_top_glass_brown.png
mv -f $MC_SOURCE/green_stained_glass_pane_top.png $DEST/xpanes_top_glass_green.png
mv -f $MC_SOURCE/cyan_stained_glass_pane_top.png $DEST/xpanes_top_glass_cyan.png
mv -f $MC_SOURCE/red_stained_glass_pane_top.png $DEST/xpanes_top_glass_red.png
